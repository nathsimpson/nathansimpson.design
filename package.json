    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^4.2.1",
