    "types": "tsc",
    "lint:eslint": "eslint .",
    "lint:typescript": "tsc --noEmit",
    "lint:next": "next lint",
    "lint": "yarn lint:eslint && yarn lint:next && yarn lint:typescript"
  },
  "license": "ISC",
